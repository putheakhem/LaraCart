{
    "name": "puthea/laracart",
    "type": "library",
    "description": "Full functionality of Shopping Cart",
    "keywords": [
        "Puthea",
        "LaraCart"
    ],
    "homepage": "https://github.com/Puthea/LaraCart",
    "license": "MIT",
    "authors": [
        {
            "name": "Puthea Khem",
            "email": "puthea.khem@gmail.coom",
            "homepage" : "http://www.khemputhea.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Puthea\\LaraCart\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Puthea\\LaraCart\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "config": {
        "sort-packages": true
    }
}
